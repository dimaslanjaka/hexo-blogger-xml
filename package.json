{
  "name": "hexo-blogger-xml",
  "version": "1.0.27",
  "description": "Import xml blogger to hexo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "index.js",
    "postinstall.js",
    "gulpfile.ts",
    "CHANGELOG.md",
    "README.md",
    "*.json",
    "packages"
  ],
  "scripts": {
    "pretest": "rm -rf dist build",
    "test": "npm run build && npm run test:ts && npm run test:gulp",
    "test:ts": "npx ts-node src/test.ts",
    "test:gulp": "npx gulp",
    "eslint": "eslint .",
    "lint": "npx eslint --fix src/**/*",
    "build": "npm run build:dev && npm run build:prod",
    "build:prod": "tsc -p tsconfig.publish.json",
    "build:dev": "tsc -p tsconfig.json",
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update": "run-s update:** && npx npm-check-updates -u -x hexo,chalk && node postinstall.js"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimaslanjaka/hexo-blogger-xml.git"
  },
  "keywords": [
    "hexo",
    "blogger",
    "xml",
    "import",
    "migrate"
  ],
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com> (http://webmanajemen.com)",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "he": "^1.2.0",
    "js-prototypes": "https://github.com/dimaslanjaka/js-prototypes/tarball/master",
    "jsdom": "^21.1.0",
    "merge2": "^1.4.1",
    "moment": "^2.29.4",
    "rimraf": "^4.1.2",
    "sanitize-filename": "^1.6.3",
    "xml2js": "^0.4.23",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/he": "^1.1.2",
    "@types/hexo": "https://github.com/dimaslanjaka/nodejs-package-types/tarball/hexo",
    "@types/lodash": "^4.14.191",
    "@types/merge2": "^1.4.0",
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3.0.2",
    "@types/sax": "^1.2.4",
    "@types/xml2js": "^0.4.11",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "hexo": "^3.8.0",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  }
}